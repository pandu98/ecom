created successfully.Please <Link to=\"/signin\" >login now.</Link>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        )\r\n    };\r\n    \r\n    \r\n    \r\n    \r\n    \r\n    const errorMessage=()=>{\r\n        return(\r\n            <div className=\"row\">\r\n                <div className=\"col-md-6 offset-sm-3 text-left\">\r\n                    <div className=\"alert alert-danger\" \r\n                    style={{display : error?\"\": \"none\" }}>\r\n                        check all feilds again\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        )\r\n    };\r\n    \r\n    \r\n     const signinForm=()=>{\r\n         return (\r\n             <div className=\"row\">\r\n                 <div className=\"col-md-6 offset-sm-3 text-left\">\r\n                     <form>\r\n                     \r\n    \r\n                            <div className=\"form-group\">\r\n                             <label className=\"text-light\">Email</label>\r\n                             <input\r\n                             className=\"form-control\"\r\n                             value={email}\r\n                             onChange={handleChange(\"email\")}\r\n                             type=\"text\"\r\n                             />\r\n                             \r\n                            </div>\r\n    \r\n                            <div className=\"form-group\">\r\n                             <label className=\"text-light\">password</label>\r\n                             <input\r\n                             className=\"form-control\"\r\n                             value={password}\r\n                             onChange={handleChange(\"password\")}\r\n                             type=\"password\"\r\n                             />\r\n                             \r\n                            </div>\r\n                            <button\r\n                            onClick={onSumit}\r\n                            className=\"btn btn-success btn-block\">Submit</button>\r\n                     </form>\r\n                 </div>\r\n             </div>\r\n         )\r\n     }\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n    return(\r\n        <Base title=\"Welocme to sign in page\" description=\"A tshirt store\">\r\n            {loadingMessage()}\r\n            \r\n            {signinForm()}\r\n            <p className=\"text-center\">{JSON.stringify(values)}</p>\r\n            {performRedirect()}\r\n        </Base>\r\n    )\r\n};\r\n\r\n\r\n\r\nexport default Signup;","C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\Menu.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\Cart.js",["103"],"import React,{useState,useEffect} from \"react\";\r\nimport Base from \"./Base\";\r\nimport Card from \"./Card\";\r\nimport { loadCart } from \"./helper/carthelper\";\r\nimport PaymentB from \"./PaymentB\";\r\n\r\n\r\n\r\n\r\n\r\n\r\nconst Cart =()=>{\r\n\r\n\r\n    const [reload,setReload]=useState(false)\r\nconst [products,setProducts]=useState([])\r\n\r\nuseEffect(() =>{\r\n    setProducts(loadCart())\r\n},[reload]);\r\n\r\n\r\n\r\nconst loadAllProducts =()=>{\r\n    return(\r\n        <div>\r\n           {products.map((product,index) =>(\r\n               <Card\r\n               key={index}\r\n               product={product}\r\n               removeFromCart={true}\r\n               addtoCart={false}\r\n               reload={reload}\r\n               setReload={setReload}\r\n               />\r\n      ))}\r\n        </div>\r\n\r\n    );\r\n};\r\n\r\nconst loadCheckout =()=>{\r\n    return(\r\n        <div>\r\n            <h1>checkout</h1>\r\n        </div>\r\n\r\n    );\r\n};\r\n\r\n\r\n\r\n    return (\r\n        <Base title=\"Cart page\" description=\"Welcome to Checkout\">\r\n           <div className=\"row text-center\">\r\n               <div className=\"col-6\">\r\n 