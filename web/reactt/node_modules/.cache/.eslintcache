tate(false);\r\n\r\n  const loadAllProducts = () => {\r\n    getProducts()\r\n      .then((data) => {\r\n        if (data.error) {\r\n          setError(data.error);\r\n          console.log(error);\r\n        } else {\r\n          setProducts(data);\r\n        }\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    loadAllProducts();\r\n  }, []);\r\n\r\n  return (\r\n  <Base title=\"Home page\" description=\"Welcome to Tshirt store\" >\r\n      <h1>Home component</h1>\r\n      <div className=\"row\">\r\n        {products.map((product, index) => {\r\n          return (\r\n            <div key={index} className=\"col-4 mb-4\" >\r\n    <Card product={product} />\r\n            </div>\r\n          );\r\n        })}\r\n      </div>\r\n  </Base>\r\n  );\r\n}\r\n","C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\helper\\coreapicalls.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\backend.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\Base.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\Card.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\helper\\imagehelper.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\core\\helper\\carthelper.js",["98"],"export const addItemToCart=(item,next)=>{\r\n    let cart=[]\r\n    if(typeof window !==undefined)\r\n    {\r\n        if(localStorage.getItem(\"cart\")){\r\n\r\n            cart=JSON.parse(localStorage.getItem(\"cart\"))\r\n\r\n        }\r\n\r\n        cart.push({\r\n            ...item\r\n        });\r\n        localStorage.setItem(\"cart\",JSON.stringify(cart));\r\n        next();\r\n    }\r\n};\r\n\r\n\r\nexport const loadCart=()=>{\r\n    if(typeof window !==undefined)\r\n    {\r\n        if(localStorage.getItem(\"cart\")){\r\n\r\n            return (JSON.parse(localStorage.getItem(\"cart\")))\r\n\r\n        }\r\n\r\n    }}\r\n\r\n    export const removeItemFromCart= productID =>{\r\n        let cart=[]\r\n        if(typeof window !==undefined)\r\n        {\r\n            if(localStorage.getItem(\"cart\")){\r\n    \r\n                cart=JSON.parse(localStorage.getItem(\"cart\"))\r\n    \r\n            }\r\n            cart.map((product,i)=>{\r\n                if(product.id===productID){\r\n                    cart.splice(i,1)\r\n                }\r\n            })\r\n            localStorage.setItem(\"cart\",JSON.stringify(cart));\r\n        }\r\n    return cart;\r\n};\r\n\r\n\r\nexport const cartEmpty=next=>{\r\n    if(typeof window !==undefined)\r\n    {\r\n        localStorage.removeItem(\"cart\");\r\n        let cart=[]\r\n        localStorage.setItem(\"cart\",JSON.stringify(cart));\r\n    }\r\n}","C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\auth\\helper\\PrivateRouters.js",["99"],"import React,{Component} from 'react'\r\nimport {Route,Redirect} from \"react-router-dom\"\r\nimport {isAuthenticated} from \"./index\";\r\n\r\n\r\nconst PrivateRoutes=({component:Component,...rest})=>{\r\n    return(\r\n       <Route\r\n       {...rest}\r\n       render={(props)=>\r\n    isAuthenticated()\r\n    ?(\r\n        <Component{...props}/>\r\n    )\r\n    :\r\n    (\r\n        <Redirect\r\n        to={{\r\n            pathname:\"/signin\",\r\n            state:{from:props.loaction}\r\n        }}\r\n        />\r\n    )\r\n    \r\n    \r\n    \r\n    }/>\r\n       \r\n    )\r\n}\r\n\r\nexport default PrivateRoutes","C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\user\\Signup.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\auth\\helper\\index.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\user\\UserDashBoard.js",[],"C:\\Users\\prabh\\OneDrive\\desktop\\web develop\\lcodev\\projfrontend\\src\\user\\Signin.js",["100","101","102"],"